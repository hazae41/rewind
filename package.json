{
  "type": "module",
  "name": "@hazae41/rewind",
  "description": "Just-in-time Tailwind compiler but supply-chain hardened",
  "version": "2.3.8",
  "repository": "github:hazae41/rewind",
  "author": "hazae41",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "prepare": "deno -RW ./x.dsync.ts && deno install",
    "prepack": "rm -rf ./out && tsc && tscousin"
  },
  "files": [
    "./out"
  ],
  "exports": {
    ".": {
      "types": "./out/mod.d.ts",
      "import": "./out/mod.js"
    }
  },
  "dependencies": {
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@hazae41/tscousin": "^1.0.27",
    "@types/node": "^24.8.1",
    "typescript": "^5.9.3"
  }
}
